<div class="boxes">

		<%@players.each_with_index do |p, i|%>
		<div class="box">
		<div class="name">
			Duente_<%=i+1%>
		</div>
		<div class="submissions">
			Submissions
			<%=p.submissions%>
		</div>
		<div class="tried">
			Tried
			<%=p.tried%>
		</div>
		<div class="sovled">
			Solved
			<%=p.solved%>
		</div>
		<div class="first-submission">
			First Submission
			<%=p.first_sub%>
		</div>
		<div class="last-submission">
			Last Submission
			<%=p.last_sub%>
		</div>
		</div>
		<%end%>

</div>